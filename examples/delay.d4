( Lead synth with single delay )

:: scale divisions ;

8 5Hz divisions
nip (age)
from
    (1) A Lead,
    (2) A' Lead,
    (3) E Lead,
    (4) D' Lead,
    (5) B Lead,
    (6) E low Lead,
    (7) G' Lead,
    (8) D Lead
choose

(instruments)

    :Lead (freq -- SOUND)
        low tr 2* & delay.
    ;

(filters)

    :delay (pcm -- pcm)
        keep lead_out
        lead_out 0.07s old
    ;

